{
  "name": "faviator.xyz",
  "description": "Web UI for faviator",
  "version": "1.2.3",
  "author": "Jason Yu <me@ycmjason.com>",
  "private": true,
  "scripts": {
    "test": "npm run test:js",
    "verify": "npm run clean && npm install && npm run build && npm test",
    "preversion": "git checkout master && git merge --squash dev && npm run verify",
    "version": "git add -A",
    "postversion": "git push && git push --tags && git checkout - && git merge master -X theirs -m 'Merge with master' && git push",
    "\n# BUILDING SCRIPTS:": "",
    "dev": "poi",
    "build": "poi build",
    "clean": "/bin/rm -rf dist coverage node_modules",
    "\n# DEPLOYMENT SCRIPTS:": "",
    "predeploy": "git checkout master && npm run verify",
    "deploy": "firebase deploy",
    "postdeploy": "git checkout -",
    "\n# TESTING SCRIPTS:": "",
    "test:js": "jest",
    "test:js:w": "npm run test:js -- --watch",
    "coverage": "codecov",
    "lint:js": "eslint --ext js,vue src test",
    "\n# HUSKY GIT HOOKS:": "",
    "precommit": "lint-staged",
    "\n# OTHER SCRIPTS": "",
    "grepFonts": "bash ./scripts/grepFonts.bash"
  },
  "dependencies": {
    "@faviator/create-svg-favicon": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "lodash.mapvalues": "^4.6.0",
    "node-sass": "^4.8.3",
    "param-case": "^2.1.1",
    "sass-loader": "^6.0.7",
    "vue": "^2.5.16",
    "vue-ga": "^1.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "@ycm.jason/eslint-config": "^1.2.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "^4.4.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "poi": "^9.6.13",
    "robotstxt-webpack-plugin": "^4.0.1",
    "vue-jest": "^2.3.0",
    "vue-template-compiler": "^2.5.16"
  }
}
