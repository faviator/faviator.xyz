{
  "name": "faviator.xyz",
  "description": "Web UI for faviator",
  "version": "1.0.0",
  "author": "Jason Yu <me@ycmjason.com>",
  "private": true,
  "scripts": {
    "test": "npm run test:js",
    "preversion": "git checkout master && git merge --squash dev && npm test",
    "version": "git add -A",
    "postversion": "git checkout - && git merge master -m 'Merge with master' && git push",
    "\n# BUILDING SCRIPTS:": "",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "\n# DEPLOYMENT SCRIPTS:": "",
    "predeploy": "git checkout master",
    "deploy": "git push && git push --tags",
    "postdeploy": "git checkout - && git push",
    "\n# TESTING SCRIPTS:": "",
    "test:js": "jest",
    "lint:js": "eslint .",
    "\n# HUSKY GIT HOOKS:": "",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "faviator": "^0.1.1",
    "vue": "^2.3.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@ycm.jason/eslint-config": "^1.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.5.1",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.17.0",
    "eslint-plugin-vue": "^4.2.2",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jest-serializer-vue": "^0.3.0",
    "lint-staged": "^6.1.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.6",
    "vue-jest": "^2.0.0",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.3.3",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.1"
  }
}
