{
  "name": "faviator.xyz",
  "description": "Web UI for faviator",
  "version": "2.1.1",
  "author": "Jason Yu <me@ycmjason.com>",
  "private": true,
  "scripts": {
    "verify": "npm run clean && npm install && npm run build",
    "preversion": "npm run verify && $EDITOR CHANGELOG.md",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "\n# BUILDING SCRIPTS:": "",
    "dev": "webpack-dev-server",
    "build": "webpack --env.prod",
    "clean": "/bin/rm -rf dist",
    "types": "tsc --noEmit",
    "\n# OTHER SCRIPTS": "",
    "grepFonts": "bash ./scripts/grepFonts.bash",
    "format": "prettier --write 'src/**/*.{js,vue}'"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "dependencies": {
    "@faviator/create-svg-favicon": "^1.2.0",
    "@ycm.jason/svg-to-img": "^0.1.4",
    "param-case": "^3.0.3",
    "vue": "^3.0.0-beta.13",
    "vue-router": "^4.0.0-alpha.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@vue/compiler-sfc": "^3.0.0-beta.13",
    "@ycm.jason/eslint-config": "^1.3.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.2",
    "vue-loader": "^16.0.0-beta.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
