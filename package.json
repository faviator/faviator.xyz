{
  "name": "faviator.xyz",
  "description": "Web UI for faviator",
  "version": "1.0.2",
  "author": "Jason Yu <me@ycmjason.com>",
  "private": true,
  "scripts": {
    "start": "superstatic --port ${PORT:-8080} --host 0.0.0.0",
    "test": "npm run test:js",
    "preversion": "git checkout master && git merge --squash dev && npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags && git checkout - && git merge master -m 'Merge with master' && git push",
    "\n# BUILDING SCRIPTS:": "",
    "dev": "poi",
    "build": "poi build",
    "clean": "/bin/rm -r dist coverage node_modules",
    "heroku-postbuild": "npm run build",
    "\n# DEPLOYMENT SCRIPTS:": "",
    "predeploy": "git checkout master",
    "deploy": "git push prod master",
    "postdeploy": "git checkout - && git push",
    "\n# TESTING SCRIPTS:": "",
    "test:js": "jest",
    "test:js:w": "npm run test:js -- --watch",
    "coverage": "codecov",
    "lint:js": "eslint --ext js,vue src test",
    "\n# HUSKY GIT HOOKS:": "",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@faviator/create-svg-favicon": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "lodash.mapvalues": "^4.6.0",
    "node-sass": "^4.7.2",
    "param-case": "^2.1.1",
    "sass-loader": "^6.0.6",
    "superstatic": "^5.0.1",
    "vue": "^2.5.13",
    "vue-ga": "^1.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "@ycm.jason/eslint-config": "^1.1.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "codecov": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-plugin-vue": "^4.3.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "poi": "^9.6.13",
    "robotstxt-webpack-plugin": "^4.0.1",
    "vue-jest": "^2.1.0",
    "vue-template-compiler": "^2.5.13"
  }
}
