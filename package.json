{
  "name": "faviator.xyz",
  "version": "1.2.4",
  "private": true,
  "scripts": {
    "test": "npm run test:unit",
    "preversion": "git checkout master && git merge --squash dev && npm run verify",
    "version": "git add -A",
    "postversion": "git push && git push --tags && git checkout - && git merge master -X theirs -m 'Merge with master' && git push",
    "\n# BUILDING SCRIPTS:": "",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "verify": "npm run clean && npm install && npm run build && npm test",
    "clean": "/bin/rm -rf dist coverage node_modules",
    "coverage": "codecov",
    "\n# DEPLOYMENT SCRIPTS:": "",
    "predeploy": "npm run verify",
    "deploy": "firebase use default && firebase deploy"
  },
  "dependencies": {
    "@faviator/create-svg-favicon": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "lodash.mapvalues": "^4.6.0",
    "param-case": "^2.1.1",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.16",
    "vue-ga": "^1.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.10",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.10",
    "@vue/test-utils": "^1.0.0-beta.10",
    "@ycm.jason/eslint-config": "^1.2.2",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "robotstxt-webpack-plugin": "^4.0.1",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
