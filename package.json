{
  "name": "faviator.xyz",
  "description": "Web UI for faviator",
  "version": "1.3.2",
  "author": "Jason Yu <me@ycmjason.com>",
  "private": true,
  "scripts": {
    "verify": "npm run clean && npm install && npm run build",
    "preversion": "npm run verify && $EDITOR CHANGELOG.md",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "\n# BUILDING SCRIPTS:": "",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --out-dir dist",
    "clean": "/bin/rm -rf dist",
    "\n# OTHER SCRIPTS": "",
    "lint:js": "eslint --ext js,vue src test",
    "grepFonts": "bash ./scripts/grepFonts.bash",
    "format": "prettier --write 'src/**/*.{js,vue}'"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "dependencies": {
    "@faviator/create-svg-favicon": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "components": "^0.1.0",
    "lodash.mapvalues": "^4.6.0",
    "node-sass": "^4.12.0",
    "param-case": "^2.1.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-ga": "^1.1.0",
    "vue-hot-reload-api": "^2.3.3",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "@ycm.jason/eslint-config": "^1.3.1",
    "codecov": "^3.5.0",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "vue-template-compiler": "^2.6.10"
  },
  "alias": {
    "@*": "./src"
  }
}
